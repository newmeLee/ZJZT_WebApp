<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="zjzt.entity">
	<class name="Project" table="t_project">
		<id name="id" column="id">
			<generator class="native"></generator>
		</id>
		<property name="projectName" column="project_name"></property>
		<property name="customeName" column="custome_name"></property>
		<property name="projectContent" column="project_content"></property>
		<property name="picUrl1" column="pic_url1"></property>
		<property name="picUrl2" column="pic_url2"></property>
		<property name="picUrl3" column="pic_url3"></property>
	</class>
</hibernate-mapping>
